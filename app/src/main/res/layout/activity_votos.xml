<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Votos">

    <TextView
        android:id="@+id/txt_titulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="106dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="106dp"
        android:text="Recuento de votos"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seek_bar2"
        android:layout_width="299dp"
        android:layout_height="34dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="39dp"
        android:max="100"
        android:progress="25"
        android:progressDrawable="@drawable/progress_track"
        android:thumb="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="86dp"
        android:layout_height="92dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="53dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/seek_bar2"
        app:layout_constraintEnd_toStartOf="@+id/txt_cand1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_titulo"
        app:srcCompat="@drawable/foto_martin_candanedo" />

    <TextView
        android:id="@+id/txt_cand1"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginEnd="138dp"
        android:gravity="center_vertical"
        android:paddingStart="0dp"
        android:paddingEnd="0dp"


    <TextView
        android:id="@+id/tv_percent2"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_marginStart="47dp"
        android:gravity="center_vertical"
        android:paddingStart="0dp"
        android:paddingEnd="0dp"


    <SeekBar
        android:id="@+id/seek_bar3"
        android:layout_width="306dp"
        android:layout_height="41dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="42dp"
        android:max="100"
        android:progress="25"
        android:progressDrawable="@drawable/progress_track"
        android:thumb="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_percent1"
        tools:layout_editor_absoluteY="584dp" />

    <TextView
        android:id="@+id/tv_percent1"
        android:layout_width="28dp"
        android:layout_height="41dp"
        android:layout_marginStart="35dp"
        android:gravity="center_vertical"
        android:paddingStart="0dp"
        android:paddingEnd="0dp"


    <TextView
        android:id="@+id/txt_cand3"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginEnd="116dp"
        android:gravity="center_vertical"
        android:paddingStart="0dp"
        android:paddingEnd="0dp"

    <Button
        android:id="@+id/btn_salir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="162dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="38dp"
        android:text="Salir"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seek_bar3" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="88dp"
        android:layout_height="94dp"
        android:layout_marginTop="53dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toTopOf="@+id/tv_percent3"
        app:layout_constraintEnd_toStartOf="@+id/txt_cand2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seek_bar2"
        app:srcCompat="@drawable/foto_omar_aizpurua" />

    <SeekBar
        android:id="@+id/seek_bar1"
        android:layout_width="300dp"
        android:layout_height="33dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="45dp"
        android:max="100"
        android:progress="25"
        android:progressDrawable="@drawable/progress_track"
        android:thumb="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_percent3"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <TextView
        android:id="@+id/txt_cand2"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginEnd="108dp"
        android:gravity="center_vertical"
        android:paddingStart="0dp"
        android:paddingEnd="0dp"

    <TextView
        android:id="@+id/tv_percent3"
        android:layout_width="27dp"
        android:layout_height="34dp"
        android:layout_marginStart="39dp"
        android:gravity="center_vertical"
        android:paddingStart="0dp"
        android:paddingEnd="0dp"


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="85dp"
        android:layout_height="90dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintEnd_toStartOf="@+id/txt_cand3"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/foto_vivian_valenzuela"
        tools:layout_editor_absoluteY="482dp" />

</androidx.constraintlayout.widget.ConstraintLayout>