        tools:layout_editor_absoluteY="584dp" />

    <TextView
        android:id="@+id/tv_percent1"
        android:layout_width="28dp"
        android:layout_height="41dp"
        android:gravity="center_vertical"
        android:paddingStart="0dp"
        android:paddingEnd="0dp"
        android:text="25%"
